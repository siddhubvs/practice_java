public class Main {

    public static void main(String[] args) {
        int i, j, counter;
        System.out.println("prime numbers from 1 to 50 are ");
        for (i = 1; i <= 50; i++) {
            counter = 0;
            for (j = 1; j <= i; j++) {
                if (i % j == 0)
                    counter++;
            }
            if(counter==2)
                System.out.println(i);
        }

    }
}
